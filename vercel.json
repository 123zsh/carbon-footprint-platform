{
  "functions": {
    "app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.py"
    }
  ],
  "builds": [
    {
      "src": "*.py",
      "use": "@vercel/python"
    }
  ],
  "buildCommand": "pip install -r requirements.txt",
  "installCommand": "pip install -r requirements.txt",
  "outputDirectory": "",
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
