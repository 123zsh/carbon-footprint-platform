{
  "version": 2,
  "builds": [
    {
      "src": "requirements.txt",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9.13"
      }
    },
    {
      "src": "app.py",
      "use": "@vercel/python@3.9"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
